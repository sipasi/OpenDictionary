<?xml version="1.0" encoding="UTF-8" ?>
<?xaml-comp compile="true" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    
    xmlns:fonts="clr-namespace:OpenDictionary.Styles.Fonts.Icons;assembly=OpenDictionary.Styles.Fonts"
     
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"> 
    <Style x:Key="DeleteImageStyle" TargetType="ImageButton">
        <Setter Property="Source">
            <fonts:FontIconExtension Icon="DeleteBin" Color="{StaticResource Red-500}" Size="32"/>
        </Setter>
    </Style>
</ResourceDictionary>
