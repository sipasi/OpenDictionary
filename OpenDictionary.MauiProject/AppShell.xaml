<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
        
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:OpenDictionary.MauiProject"
    
    xmlns:pages="clr-namespace:OpenDictionary.Views.Pages"

    Shell.FlyoutBehavior="Disabled"
     
    x:Class="OpenDictionary.MauiProject.AppShell">
    <TabBar>
        <ShellContent Title="Dictionaries" Icon="icon_book.png" ContentTemplate="{DataTemplate pages:WordGroupListPage}"/>
        <ShellContent Title="Settings" Icon="icon_settings.png" ContentTemplate="{DataTemplate pages:SettingsPage}" />
    </TabBar>
</Shell>
