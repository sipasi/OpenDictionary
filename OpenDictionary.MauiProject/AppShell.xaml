<?xml version="1.0" encoding="UTF-8" ?>
<Shell
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
        
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
    xmlns:local="clr-namespace:OpenDictionary.MauiProject"
    
    xmlns:pages="clr-namespace:OpenDictionary.Views.Pages"
    xmlns:settings="clr-namespace:OpenDictionary.Settings.Pages"
 
    x:Class="OpenDictionary.MauiProject.AppShell">
    <TabBar>
        <ShellContent x:Name="list"     Title="Dictionaries" Icon="icon_book.png" ContentTemplate="{DataTemplate pages:WordGroupListPage}"/>
        <ShellContent x:Name="settings" Title="Settings" Icon="icon_settings.png" ContentTemplate="{DataTemplate settings:SettingsPage}" />
    </TabBar>
</Shell>
