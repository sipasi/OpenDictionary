<?xml version="1.0" encoding="utf-8" ?>
<ResourceDictionary 
    xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
    xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
    x:Class="OpenDictionary.Styles.Themes.Resources.DraculaTheme">
    <Color x:Key="Primary">#6272a4</Color>
    <Color x:Key="Surface">#282a36</Color>
    <Color x:Key="Background">#44475a</Color>
    <Color x:Key="Error">Red</Color>

    <Color x:Key="SecondaryText">#ced4da</Color>

    <Color x:Key="OnPrimary">#f8f8f2</Color>
    <Color x:Key="OnSurface">#f8f8f2</Color>
    <Color x:Key="OnBackground">#f8f8f2</Color>
    <Color x:Key="OnError">Red</Color>
</ResourceDictionary>